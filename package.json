{
  "name": "messenger-webhook",
  "version": "1.0.0",
  "description": "To POC messenger webhook with nodejs",
  "main": "./index.ts",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly ./index.ts",
